{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "routify -b && rollup -c",
    "dev": "run-p dev:*",
    "dev:rollup": "rollup -c -w",
    "dev:routify": "routify",
    "dev:firebase": "firebase emulators:start --import .save-data --export-on-exit",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@roxi/routify": "^2.18.0",
    "@tsconfig/svelte": "^2.0.0",
    "@types/lodash-es": "^4.17.4",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.7.4",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "routify": {
    "dynamicImports": false
  },
  "dependencies": {
    "firebase": "^9.0.1",
    "lodash-es": "^4.17.21",
    "xml2json-light-es6module": "^2.0.5"
  }
}
