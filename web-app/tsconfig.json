{
  "extends": "./node_modules/@tsconfig/svelte/tsconfig.json",
  "include": ["src/**/*"],
  "exclude": ["node_modules/*", "__sapper__/*", "public/*"],
  "compilerOptions": {
    "resolveJsonModule": true,
    "target": "ES6",
    "baseUrl": ".",
    "paths": {
      "@form/*": ["src/form/*"],
      "@app/*": ["src/app/*"]
    }
  }
}